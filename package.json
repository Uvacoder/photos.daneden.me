{
  "name": "photos",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "flow-bin": "^0.87.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-scripts": "1.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "dist-exiftool": "^10.33.1",
    "node-exiftool": "^2.3.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-imgix": "^7.1.1",
    "react-snapshot": "^1.0.4",
    "react-waypoint": "^8.0.1"
  },
  "scripts": {
    "prestart": "node ./src/runtime/exif.js",
    "start": "react-scripts start",
    "prebuild": "node ./src/runtime/exif.js",
    "build": "react-scripts build && react-snapshot",
    "eject": "react-scripts eject",
    "flow": "flow",
    "test": "flow"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
