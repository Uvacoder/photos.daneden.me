{
  "name": "photos",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "husky": "4.0.7",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "dist-exiftool": "10.53.0",
    "node-exiftool": "2.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-imgix": "9.0.1",
    "react-snapshot": "1.3.0"
  },
  "scripts": {
    "prestart": "node ./scripts/exif.js",
    "start": "react-scripts start",
    "prebuild": "node ./scripts/exif.js",
    "build": "react-scripts build && react-snapshot",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
